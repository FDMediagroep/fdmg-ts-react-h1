{
  "name": "@fdmg/ts-react-h1",
  "version": "1.0.15",
  "description": "ReactJS H1 component",
  "main": "dist/H1.js",
  "types": "dist/H1.d.ts",
  "scripts": {
    "release": "npm update && npm install && npm run test && npm run lint && tsc -d",
    "test": "jest",
    "lint": "tslint -p .",
    "updateSnapshot": "jest --updateSnapshot",
    "u": "npm test -- -u"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:FDMediagroep/fdmg-ts-react-h1.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Willem Liu, Alexander Eerenberg",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^24.0.5",
    "@types/node": "^11.9.4",
    "@types/react": "^16.8.3",
    "coveralls": "^3.0.2",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "^3.3.5",
    "jest": "^24.1.0",
    "jest-cli": "^24.1.0",
    "raf": "^3.4.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-test-renderer": "^16.8.2",
    "ts-jest": "24.0.0",
    "tslint": "^5.12.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "setupFiles": [
      "raf/polyfill"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "collectCoverage": true,
    "coverageDirectory": "dist/coverage",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "text-summary"
    ]
  },
  "dependencies": {}
}
